    "@castiron/style-mixins": "^1.0.6",
    "@types/howler": "^2.2.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^2.0.1",
    "babel-plugin-styled-components": "^2.0.7",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.3",
    "vite": "^3.0.7",
    "vite-plugin-dts": "^1.4.1"
  }
}
